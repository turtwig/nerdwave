[Unit]
Description=Nerdwave Internet Radio Backend
Wants=network.target
Requires=postgresql.service memcached.service memcached_ratings.service

[Service]
Type=simple
User=nerdwave
Group=www-data
WorkingDirectory=/opt/nerdwave
ExecStart=/usr/bin/env pipenv run ./nw_backend.py

[Install]
WantedBy=multi-user.target
